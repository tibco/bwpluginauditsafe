<plugin>

   <extension
         point="com.tibco.bw.design.Palette">
      <paletteDrawer
            expanded="false"
            paletteID="bw.auditsafe"
            name="%tas.palette.name"
            priority="500"
            smallIcon="icons/obj16/tas_16x16.png"
            productID="bw.auditsafe.product"
            tooltip="%tas.palette.tooltip">
      </paletteDrawer>
   </extension>

   <extension
         point="com.tibco.bw.design.ActivityType">
      <activityType
            activityTypeID="bw.auditsafe.TctaCreateTransaction"
            name="%tas.activity.TctaCreateTransaction.name"
            priority="100"
            tooltip="%tas.activity.TctaCreateTransaction.tooltip"
            visible="true">
         <palette
               paletteID="bw.auditsafe">
         </palette>
      </activityType>


   </extension>

   <extension
         point="com.tibco.bw.design.ActivitySignature">
      <signature
            activityTypeID="bw.auditsafe.TctaCreateTransaction"
            signatureClass="com.tibco.bw.palette.tas.design.createtransaction.TctaCreateTransactionSignature">
      </signature>
   </extension>

   <extension
         point="com.tibco.bw.design.ActivityTypeConfigEMF">
      <config
            activityTypeID="bw.auditsafe.TctaCreateTransaction"
            eClassName="TctaCreateTransaction"
            ePackageNSURI="http://ns.tibco.com/bw/palette/tas"
            instanceName="%tas.activity.default.TctaCreateTransaction.name"
            modelHelperClass="com.tibco.bw.palette.tas.design.createtransaction.TctaCreateTransactionModelHelper">
      </config>
   </extension>

   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="com.tibco.bw.core.design.process.editor">
         <propertySection
               afterSection="com.tibco.bw.design.activity.general"
               class="com.tibco.bw.palette.tas.design.createtransaction.TctaCreateTransactionGeneralSection"
               filter="com.tibco.bw.palette.tas.design.createtransaction.TctaCreateTransactionGeneralSection"
               id="bw.auditsafe.TctaCreateTransaction.general"
               tab="tibco.bw.activity.property.tab.general">
         </propertySection>
      </propertySections>
   </extension>

   <extension
         point="com.tibco.bw.design.BWContextHelp">
      <plugin
            defaultURL="https://docs.tibco.com/pub/activematrix-businessworks-plugin-for-tas/6.0.0-february-2014/doc/html/index.html"
            id="bw.auditsafe.product"
            name="BusinessWorks TAS Plugin">
      </plugin>
   </extension>

</plugin>
